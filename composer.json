{
    "name": "damcclean/systatic",
    "description": "Systatic is a Powerful Static Site Generator for PHP Developers.",
    "authors": [
        {
            "name": "Duncan McClean",
            "email": "duncan@mcclean.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Damcclean\\Systatic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "files": [
            "src/Support/Helpers.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.2",
        "symfony/web-server-bundle": "^4.2",
        "symfony/dotenv": "^4.2",
        "jenssegers/blade": "^1.2",
        "symfony/filesystem": "^4.2",
        "damcclean/metaparsedown-extra": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^4.2"
    }
}
